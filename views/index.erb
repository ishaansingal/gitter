<table>
  <thead>
    <tr>
      <th> Repo Name </th>
      <th> Changes </th>
    </tr>
  </thead>

  <tbody>
    <% repo_contributions.each do |key, value| %>
      <tr>
        <td> <%= key %> </td>
        <td>
          <table>
            <tr>
              <th> File type </th>
              <th> Changes </th>
            </tr>
            <% value.each do |key, value| %>
              <tr>
                <td> <%= key %> </td>
                <td> <%= value %> </td>
              </tr>
            <% end %>
            </table>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

